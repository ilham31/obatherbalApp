<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".analysisJava.prediction.chooseMethodCompound.chooseMethodCompound">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textPilihjenisCompound"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Choose Method :"
            />

        <Spinner
            android:layout_below="@id/textPilihjenisCompound"
            android:id="@+id/pilihMetodeCompound"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

        </Spinner>

        <Button
            android:id="@+id/button_next_fragment_step_2_compound"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background_button"
            android:layout_alignParentBottom="true"
            android:text="Next"
            android:textAllCaps="false"
            android:textColor="@android:color/white" />

    </RelativeLayout>

</FrameLayout>